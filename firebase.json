{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  },
  "headers": [
    {
      "source": "**/*.@(gz)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        },
        {
          "key": "content-encoding",
          "value": "gzip"
        }
      ]
    }
  ]
}
